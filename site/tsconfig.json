{
	"compilerOptions": {
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		// "module": "ES6",
		// "module": "AMD",
		"module": "ESNext",
		"moduleResolution": "Node",
		// "noEmit": false,
		"noImplicitAny": true,
		"outDir": "./public/js",
		"skipLibCheck": true,
		"strict": true,
		// "target": "ES2016"
		"target": "ESNext"
	},
	"include": ["./resources/**/*.ts", "./tests/Ts/**/*.ts"]
}
